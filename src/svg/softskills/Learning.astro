<svg xmlns="http://www.w3.org/2000/svg"
     viewBox="0 0 100 100"
     width="100" height="100"
     stroke="#ffffff" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"
     fill="none">
  <!-- conexiones -->
  <!-- capa 1 a capa 2 -->
  <line x1="20" y1="25" x2="50" y2="20"/>
  <line x1="20" y1="25" x2="50" y2="40"/>
  <line x1="20" y1="50" x2="50" y2="20"/>
  <line x1="20" y1="50" x2="50" y2="40"/>
  <line x1="20" y1="75" x2="50" y2="20"/>
  <line x1="20" y1="75" x2="50" y2="40"/>
  <!-- capa 2 a capa 3 -->
  <line x1="50" y1="20" x2="80" y2="30"/>
  <line x1="50" y1="20" x2="80" y2="60"/>
  <line x1="50" y1="40" x2="80" y2="30"/>
  <line x1="50" y1="40" x2="80" y2="60"/>
  <!-- nodos -->
  <!-- capa 1 -->
  <circle cx="20" cy="25" r="6" fill="#ffffff"/>
  <circle cx="20" cy="50" r="6" fill="#ffffff"/>
  <circle cx="20" cy="75" r="6" fill="#ffffff"/>
  <!-- capa 2 -->
  <circle cx="50" cy="20" r="6" fill="#ffffff"/>
  <circle cx="50" cy="40" r="6" fill="#ffffff"/>
  <!-- capa 3 -->
  <circle cx="80" cy="30" r="6" fill="#ffffff"/>
  <circle cx="80" cy="60" r="6" fill="#ffffff"/>
</svg>